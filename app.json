{
  "expo": {
    "name": "Simvic | Scrap Metal",
    "slug": "Simvic-Scrap-Metal",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "simvic",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0000fe"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.techys.simvic",
      "runtimeVersion": {
        "policy": "appVersion"
      },
      "buildNumber": "2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.techys.simvic",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ce4669f6-92ae-4329-86bd-806dd7c7ab65"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/ce4669f6-92ae-4329-86bd-806dd7c7ab65"
    }
  }
}
